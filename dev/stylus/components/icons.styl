        url('../fonts/alepht.svg?74vzgc#alepht') format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^='icon-'], [class*='icon-'], span[class^='tag-'], span[class*='tag-'] {
    /* use !important to prevent issues with browser extensions that change fonts */
