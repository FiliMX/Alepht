li[class*='list-'],
ol[class*='list-']
    list-style none
    margin 0
    padding 0
    
.font-titillium
    font-family $font-titillium
    
.font-lato
    font-family $font-lato
    
div[class*="flex-"],
article[class="flex-"],
section[class="flex-"]
    display flex

div[class*="between"],
article[class="between"],
section[class="between"]
    justify-content space-between